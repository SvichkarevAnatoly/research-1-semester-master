@article{Seiwert08,
    abstract = {Future treatments for chronic {HCV} infection are likely to include agents that target viral components directly. Here, the preclinical characteristics of {ITMN}-191, a peptidomimetic inhibitor of the {NS3}/{4A} protease of {HCV}, are described. {ITMN}-191 inhibited a reference genotype 1 {NS3}/{4A} protein in a time dependent fashion; a hallmark of an inhibitor with a two-step binding mechanism and a slow dissociation rate. Under pre-equilibrium conditions 290 {pM} {ITMN}-191 half-maximally inhibited the reference {NS3}/{4A} protease, but a 35,000-fold higher concentration did not appreciably inhibit a panel of 79 proteases, ion channels, transporters and cell surface receptors. Sub-nanomolar biochemical potency was maintained against {NS3}/{4A} derived from {HCV} genotypes 4, 5 and 6, while single-digit nanomolar potency was observed against {NS3}/{4A} from genotypes 2b and 3a. Dilution of preformed enzyme inhibitor complex indicated {ITMN}-191 remained bound to and inhibited {NS3}/{4A} for more than 5 hours after its initial association. In cell-based potency assays, half-maximal reduction of genotype 1b {HCV} replicon {RNA} was afforded by 1.8 {nM}; 45 {nM} eliminated {HCV} replicon from cells. Peginterferon alfa-2a displayed a significant degree of antiviral synergy with {ITMN}-191 and reduced the concentration of {ITMN}-191 required for {HCV} replicon elimination. A 30 mg/kg oral dose administered to rat or monkey yielded liver concentrations 12 hours after dosing that exceeded the {ITMN}-191 concentration required to eliminate replicon {RNA} from cells. These preclinical characteristics compare favorably to other inhibitors of {NS3}/{4A} in clinical development and therefore support the clinical investigation of {ITMN}-191 for the treatment of chronic hepatitis C. {10.1128/AAC}.00699-08},
    author = {Seiwert, Scott D. and Andrews, Steven W. and Jiang, Yutong and Serebryany, Vladimir and Tan, Hua and Kossen, Karl and Rajagopalan, Ravi P. T. and Misialek, Shawn and Stevens, Sarah K. and Stoycheva, Antitsa and Hong, Jin and Lim, Sharlene R. and Qin, Xiaoli and Rieger, Robert and Condroski, Kevin R. and Zhang, Hailong and Do, Mary G. and Lemieux, Christine and Hingorani, Gary P. and Hartley, Dylan P. and Josey, John A. and Pan, Lin and Beigelman, Leonid and Blatt, Lawrence M.},
    citeulike-article-id = {3359441},
    citeulike-linkout-0 = {http://dx.doi.org/10.1128/aac.00699-08},
    citeulike-linkout-1 = {http://view.ncbi.nlm.nih.gov/pubmed/18824605},
    citeulike-linkout-2 = {http://www.hubmed.org/display.cgi?uids=18824605},
    day = {29},
    doi = {10.1128/aac.00699-08},
    journal = {Antimicrob. Agents Chemother.},
    keywords = {hcv, itmn-191},
    month = "sep",
    pages = {AAC.00699-08+},
    pmid = {18824605},
    posted-at = {2008-09-30 16:32:57},
    priority = {2},
    title = {Preclinical characteristics of the {HCV} {NS3}/{4A} protease inhibitor {ITMN}-191 (R7227)},
    url = {http://dx.doi.org/10.1128/aac.00699-08},
    year = {2008}
}

